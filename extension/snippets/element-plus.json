{
  "Vue3 Element Search Table": {
    "prefix": "v3el-search-table",
    "body": [
      "<script setup lang=\"ts\">",
      "import { ref, reactive } from 'vue'",
      "",
      "interface SearchForm {",
      "\t${1:keyword}: string",
      "\t${2:status}: string",
      "\t${3:date}: string[]",
      "}",
      "",
      "const searchForm = reactive<SearchForm>({",
      "\t${1:keyword}: '',",
      "\t${2:status}: '',",
      "\t${3:date}: []",
      "})",
      "",
      "const tableData = ref([])",
      "const loading = ref(false)",
      "const pagination = reactive({",
      "\ttotal: 0,",
      "\tcurrentPage: 1,",
      "\tpageSize: 10",
      "})",
      "",
      "const handleSearch = async () => {",
      "\ttry {",
      "\t\tloading.value = true",
      "\t\t// TODO: 实现搜索逻辑",
      "\t\tconst { data, total } = await fetchData({",
      "\t\t\t...searchForm,",
      "\t\t\tpage: pagination.currentPage,",
      "\t\t\tpageSize: pagination.pageSize",
      "\t\t})",
      "\t\ttableData.value = data",
      "\t\tpagination.total = total",
      "\t} finally {",
      "\t\tloading.value = false",
      "\t}",
      "}",
      "",
      "const handleReset = () => {",
      "\tsearchForm.${1:keyword} = ''",
      "\tsearchForm.${2:status} = ''",
      "\tsearchForm.${3:date} = []",
      "\tpagination.currentPage = 1",
      "\thandleSearch()",
      "}",
      "",
      "const handleSizeChange = (val: number) => {",
      "\tpagination.pageSize = val",
      "\thandleSearch()",
      "}",
      "",
      "const handleCurrentChange = (val: number) => {",
      "\tpagination.currentPage = val",
      "\thandleSearch()",
      "}",
      "</script>",
      "",
      "<template>",
      "\t<div class=\"search-table-container\">",
      "\t\t<!-- 搜索表单 -->",
      "\t\t<el-form :model=\"searchForm\" inline>",
      "\t\t\t<el-form-item label=\"${4:关键词}\">",
      "\t\t\t\t<el-input",
      "\t\t\t\t\tv-model=\"searchForm.${1:keyword}\"",
      "\t\t\t\t\tplaceholder=\"请输入${4:关键词}\"",
      "\t\t\t\t\tclearable",
      "\t\t\t\t\t@keyup.enter=\"handleSearch\"",
      "\t\t\t\t/>",
      "\t\t\t</el-form-item>",
      "",
      "\t\t\t<el-form-item label=\"${5:状态}\">",
      "\t\t\t\t<el-select",
      "\t\t\t\t\tv-model=\"searchForm.${2:status}\"",
      "\t\t\t\t\tplaceholder=\"请选择${5:状态}\"",
      "\t\t\t\t\tclearable",
      "\t\t\t\t>",
      "\t\t\t\t\t<el-option label=\"选项1\" value=\"1\" />",
      "\t\t\t\t\t<el-option label=\"选项2\" value=\"2\" />",
      "\t\t\t\t</el-select>",
      "\t\t\t</el-form-item>",
      "",
      "\t\t\t<el-form-item label=\"${6:日期}\">",
      "\t\t\t\t<el-date-picker",
      "\t\t\t\t\tv-model=\"searchForm.${3:date}\"",
      "\t\t\t\t\ttype=\"daterange\"",
      "\t\t\t\t\trange-separator=\"至\"",
      "\t\t\t\t\tstart-placeholder=\"开始日期\"",
      "\t\t\t\t\tend-placeholder=\"结束日期\"",
      "\t\t\t\t/>",
      "\t\t\t</el-form-item>",
      "",
      "\t\t\t<el-form-item>",
      "\t\t\t\t<el-button type=\"primary\" @click=\"handleSearch\">",
      "\t\t\t\t\t搜索",
      "\t\t\t\t</el-button>",
      "\t\t\t\t<el-button @click=\"handleReset\">重置</el-button>",
      "\t\t\t</el-form-item>",
      "\t\t</el-form>",
      "",
      "\t\t<!-- 表格 -->",
      "\t\t<el-table",
      "\t\t\tv-loading=\"loading\"",
      "\t\t\t:data=\"tableData\"",
      "\t\t\tborder",
      "\t\t\tstripe",
      "\t\t\tstyle=\"width: 100%\"",
      "\t\t>",
      "\t\t\t<el-table-column prop=\"id\" label=\"ID\" width=\"80\" />",
      "\t\t\t<el-table-column prop=\"name\" label=\"名称\" />",
      "\t\t\t<el-table-column prop=\"status\" label=\"状态\">",
      "\t\t\t\t<template #default=\"{ row }\">",
      "\t\t\t\t\t<el-tag :type=\"row.status === '1' ? 'success' : 'info'\">",
      "\t\t\t\t\t\t{{ row.status === '1' ? '正常' : '禁用' }}",
      "\t\t\t\t\t</el-tag>",
      "\t\t\t\t</template>",
      "\t\t\t</el-table-column>",
      "\t\t\t<el-table-column prop=\"date\" label=\"日期\" width=\"160\" />",
      "\t\t\t<el-table-column label=\"操作\" width=\"150\" fixed=\"right\">",
      "\t\t\t\t<template #default=\"{ row }\">",
      "\t\t\t\t\t<el-button link type=\"primary\" @click=\"handleEdit(row)\">",
      "\t\t\t\t\t\t编辑",
      "\t\t\t\t\t</el-button>",
      "\t\t\t\t\t<el-button link type=\"danger\" @click=\"handleDelete(row)\">",
      "\t\t\t\t\t\t删除",
      "\t\t\t\t\t</el-button>",
      "\t\t\t\t</template>",
      "\t\t\t</el-table-column>",
      "\t\t</el-table>",
      "",
      "\t\t<!-- 分页 -->",
      "\t\t<div class=\"pagination-container\">",
      "\t\t\t<el-pagination",
      "\t\t\t\tv-model:current-page=\"pagination.currentPage\"",
      "\t\t\t\tv-model:page-size=\"pagination.pageSize\"",
      "\t\t\t\t:page-sizes=\"[10, 20, 50, 100]\"",
      "\t\t\t\t:total=\"pagination.total\"",
      "\t\t\t\tlayout=\"total, sizes, prev, pager, next, jumper\"",
      "\t\t\t\t@size-change=\"handleSizeChange\"",
      "\t\t\t\t@current-change=\"handleCurrentChange\"",
      "\t\t\t/>",
      "\t\t</div>",
      "\t</div>",
      "</template>",
      "",
      "<style scoped>",
      ".search-table-container {",
      "\tpadding: 20px;",
      "}",
      "",
      ".pagination-container {",
      "\tmargin-top: 20px;",
      "\tdisplay: flex;",
      "\tjustify-content: flex-end;",
      "}",
      "</style>"
    ],
    "description": "Element Plus 搜索表格组合"
  },
  "Vue3 Element Form": {
    "prefix": "v3el-form",
    "body": [
      "<script setup lang=\"ts\">",
      "import { reactive, ref } from 'vue'",
      "",
      "interface FormData {",
      "\t${1:name}: string",
      "\t${2:email}: string",
      "\t${3:phone}: string",
      "\t${4:status}: string",
      "\t${5:remark}: string",
      "}",
      "",
      "const formRef = ref()",
      "const formData = reactive<FormData>({",
      "\t${1:name}: '',",
      "\t${2:email}: '',",
      "\t${3:phone}: '',",
      "\t${4:status}: '',",
      "\t${5:remark}: ''",
      "})",
      "",
      "const rules = {",
      "\t${1:name}: [{ required: true, message: '请输入名称', trigger: 'blur' }],",
      "\t${2:email}: [",
      "\t\t{ required: true, message: '请输入邮箱', trigger: 'blur' },",
      "\t\t{ type: 'email', message: '请输入正确的邮箱地址', trigger: 'blur' }",
      "\t],",
      "\t${3:phone}: [{ required: true, message: '请输入手机号', trigger: 'blur' }],",
      "\t${4:status}: [{ required: true, message: '请选择状态', trigger: 'change' }]",
      "}",
      "",
      "const handleSubmit = async () => {",
      "\tif (!formRef.value) return",
      "\tawait formRef.value.validate((valid: boolean) => {",
      "\t\tif (valid) {",
      "\t\t\tconsole.log('submit:', formData)",
      "\t\t\t// TODO: 实现提交逻辑",
      "\t\t}",
      "\t})",
      "}",
      "",
      "const handleReset = () => {",
      "\tif (!formRef.value) return",
      "\tformRef.value.resetFields()",
      "}",
      "</script>",
      "",
      "<template>",
      "\t<el-form",
      "\t\tref=\"formRef\"",
      "\t\t:model=\"formData\"",
      "\t\t:rules=\"rules\"",
      "\t\tlabel-width=\"100px\"",
      "\t>",
      "\t\t<el-form-item label=\"名称\" prop=\"${1:name}\">",
      "\t\t\t<el-input v-model=\"formData.${1:name}\" placeholder=\"请输入名称\" />",
      "\t\t</el-form-item>",
      "",
      "\t\t<el-form-item label=\"邮箱\" prop=\"${2:email}\">",
      "\t\t\t<el-input v-model=\"formData.${2:email}\" placeholder=\"请输入邮箱\" />",
      "\t\t</el-form-item>",
      "",
      "\t\t<el-form-item label=\"手机号\" prop=\"${3:phone}\">",
      "\t\t\t<el-input v-model=\"formData.${3:phone}\" placeholder=\"请输入手机号\" />",
      "\t\t</el-form-item>",
      "",
      "\t\t<el-form-item label=\"状态\" prop=\"${4:status}\">",
      "\t\t\t<el-select v-model=\"formData.${4:status}\" placeholder=\"请选择状态\">",
      "\t\t\t\t<el-option label=\"启用\" value=\"1\" />",
      "\t\t\t\t<el-option label=\"禁用\" value=\"0\" />",
      "\t\t\t</el-select>",
      "\t\t</el-form-item>",
      "",
      "\t\t<el-form-item label=\"备注\" prop=\"${5:remark}\">",
      "\t\t\t<el-input",
      "\t\t\t\tv-model=\"formData.${5:remark}\"",
      "\t\t\t\ttype=\"textarea\"",
      "\t\t\t\t:rows=\"3\"",
      "\t\t\t\tplaceholder=\"请输入备注\"",
      "\t\t\t/>",
      "\t\t</el-form-item>",
      "",
      "\t\t<el-form-item>",
      "\t\t\t<el-button type=\"primary\" @click=\"handleSubmit\">提交</el-button>",
      "\t\t\t<el-button @click=\"handleReset\">重置</el-button>",
      "\t\t</el-form-item>",
      "\t</el-form>",
      "</template>"
    ],
    "description": "Element Plus 表单模板"
  }
} 