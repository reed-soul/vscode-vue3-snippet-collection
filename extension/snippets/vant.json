{
  "Vue3 Vant List": {
    "prefix": "v3vant-list",
    "body": [
      "<script setup lang=\"ts\">",
      "import { ref } from 'vue'",
      "",
      "const loading = ref(false)",
      "const finished = ref(false)",
      "const refreshing = ref(false)",
      "const list = ref<any[]>([])",
      "const page = ref(1)",
      "",
      "const onLoad = async () => {",
      "\ttry {",
      "\t\tif (refreshing.value) {",
      "\t\t\tlist.value = []",
      "\t\t\trefreshing.value = false",
      "\t\t}",
      "",
      "\t\tloading.value = true",
      "\t\tconst { data, total } = await fetchData(page.value)",
      "\t\tlist.value.push(...data)",
      "",
      "\t\tpage.value++",
      "\t\tloading.value = false",
      "\t\tfinished.value = list.value.length >= total",
      "\t} catch (error) {",
      "\t\tloading.value = false",
      "\t\tshowToast('加载失败')",
      "\t}",
      "}",
      "",
      "const onRefresh = () => {",
      "\tfinished.value = false",
      "\tpage.value = 1",
      "\tonLoad()",
      "}",
      "</script>",
      "",
      "<template>",
      "\t<van-pull-refresh v-model=\"refreshing\" @refresh=\"onRefresh\">",
      "\t\t<van-list",
      "\t\t\tv-model:loading=\"loading\"",
      "\t\t\t:finished=\"finished\"",
      "\t\t\tfinished-text=\"没有更多了\"",
      "\t\t\t@load=\"onLoad\"",
      "\t\t>",
      "\t\t\t<van-cell v-for=\"item in list\" :key=\"item.id\">",
      "\t\t\t\t<template #title>",
      "\t\t\t\t\t<div class=\"title\">{{ item.title }}</div>",
      "\t\t\t\t</template>",
      "\t\t\t\t<template #value>",
      "\t\t\t\t\t<div class=\"value\">{{ item.value }}</div>",
      "\t\t\t\t</template>",
      "\t\t\t</van-cell>",
      "\t\t</van-list>",
      "\t</van-pull-refresh>",
      "</template>"
    ],
    "description": "Vant 列表组件模板"
  },
  "Vue3 Vant Form": {
    "prefix": "v3vant-form",
    "body": [
      "<script setup lang=\"ts\">",
      "import { ref } from 'vue'",
      "import type { FormInstance } from 'vant'",
      "",
      "const formRef = ref<FormInstance>()",
      "const form = ref({",
      "\tusername: '',",
      "\tpassword: '',",
      "\tphone: '',",
      "\tverifyCode: ''",
      "})",
      "",
      "const rules = {",
      "\tusername: [{ required: true, message: '请填写用户名' }],",
      "\tpassword: [{ required: true, message: '请填写密码' }],",
      "\tphone: [",
      "\t\t{ required: true, message: '请填写手机号' },",
      "\t\t{ pattern: /^1[3-9]\\d{9}$/, message: '手机号格式错误' }",
      "\t],",
      "\tverifyCode: [{ required: true, message: '请填写验证码' }]",
      "}",
      "",
      "const onSubmit = async () => {",
      "\tif (!formRef.value) return",
      "",
      "\ttry {",
      "\t\tawait formRef.value.validate()",
      "\t\t// TODO: 提交表单",
      "\t\tshowToast('提交成功')",
      "\t} catch (error) {",
      "\t\tconsole.log('表单校验失败')",
      "\t}",
      "}",
      "</script>",
      "",
      "<template>",
      "\t<van-form ref=\"formRef\" @submit=\"onSubmit\">",
      "\t\t<van-cell-group inset>",
      "\t\t\t<van-field",
      "\t\t\t\tv-model=\"form.username\"",
      "\t\t\t\tname=\"username\"",
      "\t\t\t\tlabel=\"用户名\"",
      "\t\t\t\tplaceholder=\"请输入用户名\"",
      "\t\t\t\t:rules=\"rules.username\"",
      "\t\t\t/>",
      "\t\t\t<van-field",
      "\t\t\t\tv-model=\"form.password\"",
      "\t\t\t\ttype=\"password\"",
      "\t\t\t\tname=\"password\"",
      "\t\t\t\tlabel=\"密码\"",
      "\t\t\t\tplaceholder=\"请输入密码\"",
      "\t\t\t\t:rules=\"rules.password\"",
      "\t\t\t/>",
      "\t\t\t<van-field",
      "\t\t\t\tv-model=\"form.phone\"",
      "\t\t\t\ttype=\"tel\"",
      "\t\t\t\tname=\"phone\"",
      "\t\t\t\tlabel=\"手机号\"",
      "\t\t\t\tplaceholder=\"请输入手机号\"",
      "\t\t\t\t:rules=\"rules.phone\"",
      "\t\t\t/>",
      "\t\t\t<van-field",
      "\t\t\t\tv-model=\"form.verifyCode\"",
      "\t\t\t\tcenter",
      "\t\t\t\tname=\"verifyCode\"",
      "\t\t\t\tlabel=\"验证码\"",
      "\t\t\t\tplaceholder=\"请输入验证码\"",
      "\t\t\t\t:rules=\"rules.verifyCode\"",
      "\t\t\t>",
      "\t\t\t\t<template #button>",
      "\t\t\t\t\t<van-button size=\"small\" type=\"primary\">发送验证码</van-button>",
      "\t\t\t\t</template>",
      "\t\t\t</van-field>",
      "\t\t</van-cell-group>",
      "\t\t<div style=\"margin: 16px\">",
      "\t\t\t<van-button round block type=\"primary\" native-type=\"submit\">",
      "\t\t\t\t提交",
      "\t\t\t</van-button>",
      "\t\t</div>",
      "\t</van-form>",
      "</template>"
    ],
    "description": "Vant 表单组件模板"
  }
} 