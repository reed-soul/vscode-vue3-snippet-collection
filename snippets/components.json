{
  "Vue3 Modal Component": {
    "prefix": "v3modal",
    "body": [
      "<script setup lang=\"ts\">",
      "interface Props {",
      "\tshow: boolean",
      "\ttitle?: string",
      "}",
      "",
      "interface Emits {",
      "\t(e: 'update:show', value: boolean): void",
      "\t(e: 'confirm'): void",
      "}",
      "",
      "const props = defineProps<Props>()",
      "const emit = defineEmits<Emits>()",
      "",
      "const close = () => emit('update:show', false)",
      "const confirm = () => {",
      "\temit('confirm')",
      "\tclose()",
      "}",
      "</script>",
      "",
      "<template>",
      "\t<Teleport to=\"body\">",
      "\t\t<Transition name=\"modal\">",
      "\t\t\t<div v-if=\"show\" class=\"modal-overlay\" @click=\"close\">",
      "\t\t\t\t<div class=\"modal\" @click.stop>",
      "\t\t\t\t\t<div class=\"modal-header\">",
      "\t\t\t\t\t\t<h3>{{ title || '提示' }}</h3>",
      "\t\t\t\t\t\t<button class=\"close-btn\" @click=\"close\">&times;</button>",
      "\t\t\t\t\t</div>",
      "\t\t\t\t\t<div class=\"modal-body\">",
      "\t\t\t\t\t\t<slot></slot>",
      "\t\t\t\t\t</div>",
      "\t\t\t\t\t<div class=\"modal-footer\">",
      "\t\t\t\t\t\t<button @click=\"close\">取消</button>",
      "\t\t\t\t\t\t<button class=\"primary\" @click=\"confirm\">确定</button>",
      "\t\t\t\t\t</div>",
      "\t\t\t\t</div>",
      "\t\t\t</div>",
      "\t\t</Transition>",
      "\t</Teleport>",
      "</template>",
      "",
      "<style scoped>",
      ".modal-overlay {",
      "\tposition: fixed;",
      "\ttop: 0;",
      "\tleft: 0;",
      "\tright: 0;",
      "\tbottom: 0;",
      "\tbackground: rgba(0, 0, 0, 0.5);",
      "\tdisplay: flex;",
      "\talign-items: center;",
      "\tjustify-content: center;",
      "}",
      "",
      ".modal {",
      "\tbackground: white;",
      "\tborder-radius: 8px;",
      "\tpadding: 20px;",
      "\tmax-width: 500px;",
      "\twidth: 90%;",
      "}",
      "",
      ".modal-enter-active,",
      ".modal-leave-active {",
      "\ttransition: opacity 0.3s ease;",
      "}",
      "",
      ".modal-enter-from,",
      ".modal-leave-to {",
      "\topacity: 0;",
      "}",
      "</style>"
    ],
    "description": "Vue 3 Modal 组件模板"
  },
  "Vue3 Table Component": {
    "prefix": "v3table",
    "body": [
      "<script setup lang=\"ts\">",
      "interface Props {",
      "\tdata: any[]",
      "\tcolumns: {",
      "\t\tkey: string",
      "\t\ttitle: string",
      "\t\trender?: (row: any) => string",
      "\t}[]",
      "}",
      "",
      "const props = defineProps<Props>()",
      "",
      "const renderCell = (row: any, column: Props['columns'][0]) => {",
      "\tif (column.render) {",
      "\t\treturn column.render(row)",
      "\t}",
      "\treturn row[column.key]",
      "}",
      "</script>",
      "",
      "<template>",
      "\t<div class=\"table-container\">",
      "\t\t<table>",
      "\t\t\t<thead>",
      "\t\t\t\t<tr>",
      "\t\t\t\t\t<th v-for=\"col in columns\" :key=\"col.key\">",
      "\t\t\t\t\t\t{{ col.title }}",
      "\t\t\t\t\t</th>",
      "\t\t\t\t</tr>",
      "\t\t\t</thead>",
      "\t\t\t<tbody>",
      "\t\t\t\t<tr v-for=\"(row, index) in data\" :key=\"index\">",
      "\t\t\t\t\t<td v-for=\"col in columns\" :key=\"col.key\">",
      "\t\t\t\t\t\t{{ renderCell(row, col) }}",
      "\t\t\t\t\t</td>",
      "\t\t\t\t</tr>",
      "\t\t\t</tbody>",
      "\t\t</table>",
      "\t</div>",
      "</template>",
      "",
      "<style scoped>",
      ".table-container {",
      "\toverflow-x: auto;",
      "}",
      "",
      "table {",
      "\twidth: 100%;",
      "\tborder-collapse: collapse;",
      "}",
      "",
      "th, td {",
      "\tpadding: 12px;",
      "\ttext-align: left;",
      "\tborder-bottom: 1px solid #eee;",
      "}",
      "",
      "th {",
      "\tbackground: #f5f5f5;",
      "\tfont-weight: 600;",
      "}",
      "</style>"
    ],
    "description": "Vue 3 Table 组件模板"
  }
} 