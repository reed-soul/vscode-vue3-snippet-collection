{
  "v-for": {
    "prefix": "v3for",
    "body": "v-for=\"${1:item} in ${2:items}\" :key=\"${3:item.id}\"",
    "description": "v-for directive"
  },
  "v-if": {
    "prefix": "v3if",
    "body": "v-if=\"${1:condition}\"",
    "description": "v-if directive"
  },
  "v-model": {
    "prefix": "v3model",
    "body": "v-model=\"${1:value}\"",
    "description": "v-model directive"
  }
} 