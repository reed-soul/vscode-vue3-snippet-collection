{
  "Vue3 Antd Search Table": {
    "prefix": "v3a-search-table",
    "body": [
      "<script setup lang=\"ts\">",
      "import { ref, reactive } from 'vue'",
      "import { message } from 'ant-design-vue'",
      "",
      "interface SearchForm {",
      "\t${1:keyword}: string",
      "\t${2:status}: string",
      "\t${3:dateRange}: string[]",
      "}",
      "",
      "const searchForm = reactive<SearchForm>({",
      "\t${1:keyword}: '',",
      "\t${2:status}: '',",
      "\t${3:dateRange}: []",
      "})",
      "",
      "const tableData = ref([])",
      "const loading = ref(false)",
      "const pagination = reactive({",
      "\ttotal: 0,",
      "\tcurrent: 1,",
      "\tpageSize: 10,",
      "\tshowSizeChanger: true,",
      "\tshowTotal: (total: number) => `共 ${total} 条`",
      "})",
      "",
      "const columns = [",
      "\t{ title: 'ID', dataIndex: 'id', key: 'id' },",
      "\t{ title: '名称', dataIndex: 'name', key: 'name' },",
      "\t{",
      "\t\ttitle: '状态',",
      "\t\tdataIndex: 'status',",
      "\t\tkey: 'status',",
      "\t\tcustomRender: ({ text }: { text: string }) => (",
      "\t\t\t<a-tag color={text === '1' ? 'success' : 'default'}>",
      "\t\t\t\t{text === '1' ? '正常' : '禁用'}",
      "\t\t\t</a-tag>",
      "\t\t)",
      "\t},",
      "\t{",
      "\t\ttitle: '操作',",
      "\t\tkey: 'action',",
      "\t\twidth: 150,",
      "\t\tfixed: 'right',",
      "\t\tcustomRender: ({ record }: { record: any }) => (",
      "\t\t\t<>",
      "\t\t\t\t<a-button type=\"link\" onClick={() => handleEdit(record)}>",
      "\t\t\t\t\t编辑",
      "\t\t\t\t</a-button>",
      "\t\t\t\t<a-button type=\"link\" danger onClick={() => handleDelete(record)}>",
      "\t\t\t\t\t删除",
      "\t\t\t\t</a-button>",
      "\t\t\t</>",
      "\t\t)",
      "\t}",
      "]",
      "",
      "const handleTableChange = (pag: any) => {",
      "\tpagination.current = pag.current",
      "\tpagination.pageSize = pag.pageSize",
      "\thandleSearch()",
      "}",
      "",
      "const handleSearch = async () => {",
      "\ttry {",
      "\t\tloading.value = true",
      "\t\t// TODO: 实现搜索逻辑",
      "\t\tconst { data, total } = await fetchData({",
      "\t\t\t...searchForm,",
      "\t\t\tpage: pagination.current,",
      "\t\t\tpageSize: pagination.pageSize",
      "\t\t})",
      "\t\ttableData.value = data",
      "\t\tpagination.total = total",
      "\t} catch (error) {",
      "\t\tmessage.error('获取数据失败')",
      "\t} finally {",
      "\t\tloading.value = false",
      "\t}",
      "}",
      "",
      "const handleReset = () => {",
      "\tsearchForm.${1:keyword} = ''",
      "\tsearchForm.${2:status} = ''",
      "\tsearchForm.${3:dateRange} = []",
      "\tpagination.current = 1",
      "\thandleSearch()",
      "}",
      "</script>",
      "",
      "<template>",
      "\t<div class=\"search-table-container\">",
      "\t\t<!-- 搜索表单 -->",
      "\t\t<a-form layout=\"inline\" :model=\"searchForm\">",
      "\t\t\t<a-form-item label=\"关键词\">",
      "\t\t\t\t<a-input",
      "\t\t\t\t\tv-model:value=\"searchForm.${1:keyword}\"",
      "\t\t\t\t\tplaceholder=\"请输入关键词\"",
      "\t\t\t\t\tallow-clear",
      "\t\t\t\t\t@pressEnter=\"handleSearch\"",
      "\t\t\t\t/>",
      "\t\t\t</a-form-item>",
      "",
      "\t\t\t<a-form-item label=\"状态\">",
      "\t\t\t\t<a-select",
      "\t\t\t\t\tv-model:value=\"searchForm.${2:status}\"",
      "\t\t\t\t\tplaceholder=\"请选择状态\"",
      "\t\t\t\t\tallow-clear",
      "\t\t\t\t\tstyle=\"width: 120px\"",
      "\t\t\t\t>",
      "\t\t\t\t\t<a-select-option value=\"1\">正常</a-select-option>",
      "\t\t\t\t\t<a-select-option value=\"0\">禁用</a-select-option>",
      "\t\t\t\t</a-select>",
      "\t\t\t</a-form-item>",
      "",
      "\t\t\t<a-form-item label=\"日期范围\">",
      "\t\t\t\t<a-range-picker",
      "\t\t\t\t\tv-model:value=\"searchForm.${3:dateRange}\"",
      "\t\t\t\t\tstyle=\"width: 240px\"",
      "\t\t\t\t/>",
      "\t\t\t</a-form-item>",
      "",
      "\t\t\t<a-form-item>",
      "\t\t\t\t<a-space>",
      "\t\t\t\t\t<a-button type=\"primary\" @click=\"handleSearch\">",
      "\t\t\t\t\t\t搜索",
      "\t\t\t\t\t</a-button>",
      "\t\t\t\t\t<a-button @click=\"handleReset\">重置</a-button>",
      "\t\t\t\t</a-space>",
      "\t\t\t</a-form-item>",
      "\t\t</a-form>",
      "",
      "\t\t<!-- 表格 -->",
      "\t\t<a-table",
      "\t\t\t:columns=\"columns\"",
      "\t\t\t:data-source=\"tableData\"",
      "\t\t\t:pagination=\"pagination\"",
      "\t\t\t:loading=\"loading\"",
      "\t\t\trow-key=\"id\"",
      "\t\t\t@change=\"handleTableChange\"",
      "\t\t>",
      "\t\t</a-table>",
      "\t</div>",
      "</template>",
      "",
      "<style scoped>",
      ".search-table-container {",
      "\tpadding: 24px;",
      "}",
      "",
      ".ant-form {",
      "\tmargin-bottom: 24px;",
      "}",
      "</style>"
    ],
    "description": "Ant Design Vue 搜索表格组合"
  }
} 